        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Contractor__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Supplier</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Account.RecordTypeId</field>
                <operation>equals</operation>
                <value>Contractor</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Operator Contractor</relationshipLabel>
        <relationshipName>Contractor_Operators</relationshipName>
        <required>false</required>
        <securityClassification>InETL</securityClassification>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Days_Since_Created__c</fullName>
        <externalId>false</externalId>
        <formula>ABS(CreatedDate  - NOW())</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Days Since Created</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Initial_Payment_Made__c</fullName>
        <defaultValue>false</defaultValue>
        <description>tracking first payment</description>
        <externalId>false</externalId>
        <label>Initial Payment Made</label>
        <securityClassification>InETL</securityClassification>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Internal_Client_Account__c</fullName>
        <description>Boolean field created to identify internal Avetta accounts that have connections with supplier accounts. Field will be used in automation to disqualify records.</description>
        <externalId>false</externalId>
        <formula>CONTAINS(Client_Name__c, &apos;Avetta&apos;)
 || 
	CONTAINS(Client_Name__c, &apos;PQF&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Internal Client Account</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Active__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Tracks whether the connection is active and current</description>
        <externalId>false</externalId>
