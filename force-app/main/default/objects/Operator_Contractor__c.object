        <description>Used for Cargill Roll Up Summary field on the account.</description>
        <externalId>false</externalId>
        <formula>CONTAINS(Client_Name__c, &apos;Cargill&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Cargill Client</label>
