    <fields>
        <fullName>Expense_Costs__c</fullName>
        <description>Dining + Rental Car + Mileage + hotel + airfare + misc</description>
        <externalId>false</externalId>
        <formula>(Dining__c  *  Control_Field_Dining_Expense__c )
+  IF(   No_Rental_Car__c ,  Estimated_Mileage__c * 0.535  , Duration_of_Trip_Days__c  *  Estimated_Daily_Rental_Car_Cost__c)
+  IF( Duration_of_Trip_Days__c  &lt;&gt; 0,  Estimated_Dailey_Hotel_Costs__c  *  Duration_of_Trip_Days__c + Control_Field_Dining_Expense__c * Duration_of_Trip_Days__c , 0) 
+ Estimated_Airfare__c + Other_Costs__c

+
 (Total_JRH_Test_redo__c  * ((Dining__c  *  Control_Field_Dining_Expense__c )
+ IF( Duration_of_Trip_Days__c  &lt;&gt; 0,  Estimated_Dailey_Hotel_Costs__c  *  Duration_of_Trip_Days__c + Control_Field_Dining_Expense__c * Duration_of_Trip_Days__c, 0) 
+ Estimated_Airfare__c ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Dining + Rental Car + Mileage + hotel + airfare + misc</inlineHelpText>
        <label>Total Expense Approval Requested</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
