        <deleteConstraint>SetNull</deleteConstraint>
        <description>Who from the Solutions Consulting team do you want allocated to this opportunity ?</description>
        <externalId>false</externalId>
        <inlineHelpText>Who from the Solutions Consulting team do you want allocated to this opportunity ?
Michael Ketchmere, Charl Smit - as of May 21, 2015</inlineHelpText>
        <label>Solutions Consultant</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>1 OR 2</booleanFilter>
            <errorMessage>You must select a solution consultant</errorMessage>
            <filterItems>
