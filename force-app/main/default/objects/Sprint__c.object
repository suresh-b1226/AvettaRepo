    <fields>
        <fullName>Max_Admin_Points__c</fullName>
        <description>Total Available Admin Story points per sprint. 

Created for 02642914</description>
        <externalId>false</externalId>
        <formula>ROUND( 5 * (
( 5 * FLOOR( ( DATEVALUE( CutOffDate__c ) - DATE( 1900, 1, 8) ) / 7) +
MIN(5,
MOD( DATEVALUE( CutOffDate__c ) - DATE( 1900, 1, 8), 7) +
MIN( 1, 24 / 8 * ( MOD( CutOffDate__c - DATETIMEVALUE( &apos;1900-01-08 14:00:00&apos; ), 1 ) ) )
)
)
-
( 5 * FLOOR( ( DATEVALUE( StartDate__c ) - DATE( 1900, 1, 8) ) / 7) +
MIN( 5,
MOD( DATEVALUE( StartDate__c ) - DATE( 1996, 1, 1), 7 ) +
MIN( 1, 24 / 8 * ( MOD( StartDate__c - DATETIMEVALUE( &apos;1900-01-08 14:00:00&apos; ), 1) ) )
)
)
),
0 )
*0.7*Active_Admins__c - Avetta_Holiday_Adjustment__c - AdminPTOAdjustment__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Max Admin Points</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Max_Dev_Points__c</fullName>
        <description>Total Available Admin Story points per sprint.

Created for 02642914</description>
        <externalId>false</externalId>
        <formula>ROUND( 6 * (
( 5 * FLOOR( ( DATEVALUE( CutOffDate__c ) - DATE( 1900, 1, 8) ) / 7) +
MIN(5,
MOD( DATEVALUE( CutOffDate__c ) - DATE( 1900, 1, 8), 7) +
MIN( 1, 24 / 8 * ( MOD( CutOffDate__c - DATETIMEVALUE( &apos;1900-01-08 14:00:00&apos; ), 1 ) ) )
)
)
-
( 5 * FLOOR( ( DATEVALUE( StartDate__c ) - DATE( 1900, 1, 8) ) / 7) +
MIN( 5,
MOD( DATEVALUE( StartDate__c ) - DATE( 1996, 1, 1), 7 ) +
MIN( 1, 24 / 8 * ( MOD( StartDate__c - DATETIMEVALUE( &apos;1900-01-08 14:00:00&apos; ), 1) ) )
)
)
),
0 )
*0.8*2 - Avetta_Holiday_Adjustment__c -  PTODevAdjustment__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
