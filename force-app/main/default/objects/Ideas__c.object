    <validationRules>
        <fullName>BA_Field_Restrictions</fullName>
        <active>true</active>
        <description>Created to restrict changes to the idea once the field &apos;BA&apos;d&apos; has been set to yes.  This restriction will prevent additional items from being added to the idea after it has already been BA&apos;d.</description>
        <errorConditionFormula>AND(
				OR(	
				ISNEW(),
				ISCHANGED(BAd__c)),
				$Profile.Id &lt;&gt; &quot;00e70000000tKSw&quot;/*System Administrator*/,
				$Profile.Id &lt;&gt; &quot;00e0g000001WNfo&quot;/*Development*/,
				OR(
								ISPICKVAL(BAd__c, &apos;Yes&apos;),
								 ISPICKVAL(PRIORVALUE(BAd__c),&apos;Yes&apos;)
								))</errorConditionFormula>
        <errorDisplayField>BAd__c</errorDisplayField>
        <errorMessage>Only the Salesforce team can BA an Idea.</errorMessage>
    </validationRules>
