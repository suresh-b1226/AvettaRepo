<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <decisions>
        <name>IS_PQF_Closed</name>
        <label>IS PQF Closed</label>
        <locationX>430</locationX>
        <locationY>52</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Closed_PQF</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>PQFSubject</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Safety And Health Management System Documentation</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>PQFAccountid</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>AccountID</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>PQFIsClosed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Case_Stage</targetReference>
            </connector>
            <label>Closed PQF</label>
        </rules>
    </decisions>
    <formulas>
        <description>Used to calculate the Audit SLA</description>
        <name>AuditSLAFormula</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + {!Triggeringcase.SLA_Days__c}+1</expression>
    </formulas>
    <formulas>
        <description>Holds PQF closed date plus 180 days</description>
        <name>PQFcloseddate</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} - 150</expression>
    </formulas>
    <interviewLabel>AG PQF Verification {!$Flow.CurrentDateTime}</interviewLabel>
    <label>AG PQF Verification</label>
    <processMetadataValues>
